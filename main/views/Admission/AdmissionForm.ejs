<%- include("../partials/navbar.ejs") %>

    <div class="Admission_Form">
        <h3>New student registration form</h3>

        <form action="" method="post">
            <div class="FirstName">
                <label for="FirstName">First Name: </label>
                <input type="text" name="FirstName" id="FirstName" required placeholder="First name">
                <label for="LastName">Last Name: </label>
                <input type="text" name="LastName" id="LastName" placeholder="Last name">
            </div>
            <div>
                <label for="MotherName">Mother Name: </label>
                <input type="text" name="MotherName" id="MotherName" placeholder="Mother name" maxlength="20"
                    minlength="5">
            </div>
            <div class="sex">
                <p>Sex:
                    <input type="radio" name="sex" id="sex" value="Male">
                    <label for="male">Male</label>
                    <input type="radio" name="sex" id="sex" value="Female">
                    <label for="Female">Female </label>
                </p>
            </div>
            <div>
                <label for="PlaceBirth">Place of birth: </label>
                <input type="text" name="PlaceBirth" id="PalceBirth" placeholder="place of birth">
            </div>
            <div>
                <label for="DateBirth">Date of birth: </label>
                <input type="date" name="DateBirth" id="DateBirth">
            </div>
            <div>
                <label for="Deegaanka">Deegaanka: </label>
                <input type="text" name="Deegaanka" id="Deegaanka" placeholder="Deegaankaaga">
            </div>
            <div>
                <label for="Email">E-mail: </label>
                <input type="email" name="Email" id="Email" placeholder="E-mail">
            </div>
            <div>
                <label for="Phone">Phone number: </label>
                <input type="number" name="Phone" id="Phone" placeholder="Enter your number">
                <input type="number" name="Phone" id="Phone" placeholder="Enter your second number">
