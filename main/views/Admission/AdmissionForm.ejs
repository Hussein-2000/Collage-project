                                <option value="Electrical Engineering">Electrical Engineering</option>
                                <option value="Mechanical Engineering">Mechanical Engineering</option>
                                <option value="Archituctural Engineering">Archituctural Engineering</option>
                            </optgroup>
                            <optgroup label="Faculty of Medicine">
                                <option value="Medicine and sergury">Medicine and sergury</option>
                            </optgroup>
                            <optgroup label="Faculty of Economics">
                                <option value="Accounting">Accounting</option>
                                <option value="Bank and Finance">Bank and Finance</option>
                                <option value="Bure economics">Bure economics</option>
                                <option value="Stastistics">Stastistics</option>
                            </optgroup>
                            <optgroup label="Faculty of Veterinary">
                                <option value="Animal and husbendary">Computer science</option>
                                <option value="Animal medicine">Animal medicine</option>
                            </optgroup>
                            <optgroup label="Faculty of Agriculture">
                                <option value="Agricultural science">Agricultural science</option>
                            </optgroup>
                            <optgroup label="Faculty of Law">
                                <option value="Law">Law</option>
                            </optgroup>
                            <optgroup label="Faculty of social science">
                                <option value="Political science">Political science</option>
                                <option value="Social work">Social work</option>
                                <option value="Jouranalism">Jouranalism</option>
                            </optgroup>
                            <optgroup label="Faculty of Health Sciences">
                                <option value="Health science">Health science</option>
                            </optgroup>
                            <optgroup label="Faculty of Education">
                                <option value="Math and physics">Math and physics</option>
                                <option value="Biology and chemistry">Biology and chemistry</option>
                                <option value="Chemistry and Biology">Chemistry and Biology</option>
                                <option value="Geography and history">Geography and history</option>
                                <option value="History and english">History and english</option>
                                <option value="English and litrature">English and litrature</option>
                                <option value="Arabic and litrature">Arabic and litrature</option>
                            </optgroup>
                        </select>
                    </div>
                    <div class="Second">
